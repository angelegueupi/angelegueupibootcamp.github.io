   <!--Headings demo-->
   <h1>This is sa demo of a bulleted list</h1>
 <ul>
    <li>item</li>
    <li>another item</li>
    <li>another item again</li>
 </ul>
 <h1>This is a demo of a bulleted list with sub items</h1>
 <ul>
    <li>thing</li>
    <ul>
        <li>sub thing</li>
        <li> another bub thing</li>
    </ul>
    <li>Another thinh</li>
    <ul>
        <li> This is other sub list</li>
        <li> This is another sub thing</li>
    </ul>
 </ul>
 <h1>This is a demo of a bulleted list with a different symbol/h1>
    <ul type = "square">
        <li>Square item </li>
        <li> Another square items</li>
        <li> and another square item</li>
    </ul>
  </body>
